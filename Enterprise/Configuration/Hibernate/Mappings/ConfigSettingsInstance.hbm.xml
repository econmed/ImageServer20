<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="ClearCanvas.Enterprise.Configuration" namespace="ClearCanvas.Enterprise.Configuration">
  <class name="ConfigSettingsInstance" proxy="ConfigSettingsInstance" table="ConfigSettingsInstance_">
    <id name="OID" column="OID_" type="Int64">
      <generator class="hilo">
        <param name="max_lo">100</param>
      </generator>
    </id>
    <version name="Version" column="Version_"/>
    <many-to-one name="Group" class="ConfigSettingsGroup" column="ConfigSettingsGroupOID_" not-null="true" fetch="join"/>
    <property name="User" column="User_" type="String" length="200" not-null="true"/>
    <property name="InstanceKey" column="InstanceKey_" type="String" length="1000" not-null="false"/>
    <property name="SettingsValuesXml" column="SettingsValuesXml_" type="StringClob" length="65000" not-null="false"/>
  </class>
</hibernate-mapping>
