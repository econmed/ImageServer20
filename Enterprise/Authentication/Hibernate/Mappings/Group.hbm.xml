<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="ClearCanvas.Enterprise.Authentication" namespace="ClearCanvas.Enterprise.Authentication">
  <class name="Group" proxy="Group" table="Group_">
    <id name="OID" column="OID_" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <version name="Version" column="Version_"/>
    <property name="GroupName" column="GroupName_" type="String" length="100" not-null="true"/>
    <set name="Permissions" table="GroupPermissions_" lazy="true" cascade="all" access="nosetter.camelcase-underscore">
      <key column="GroupOID_"/>
      <many-to-many column="PermissionOID_" class="Permission" fetch="join"/>
    </set>
    <set name="Users" table="GroupUsers_" lazy="true" inverse="true" access="nosetter.camelcase-underscore">
      <key column="GroupOID_"/>
      <many-to-many column="UserOID_" class="User" fetch="join"/>
    </set>
  </class>
</hibernate-mapping>
