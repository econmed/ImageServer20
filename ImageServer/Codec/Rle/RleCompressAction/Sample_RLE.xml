<!-- 
This rule instructs the server to use RLE compression
on US and CR studies.
-->
<rule>
	<condition
	  expressionLanguage="dicom">		
			<and>
				<equal
				  test="$Modality"
				  refValue="US" />
				<equal
				  test="$Modality"
				  refValue="CR" />
			</and>
	</condition>
	<action>
		<rle/>
	</action>
</rule>