      <dependentAssembly>
        <assemblyIdentity name="ClearCanvas.ImageServer.Rules" culture="neutral"/>
        <codeBase href="plugins\ClearCanvas.ImageServer.Rules.dll"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="ClearCanvas.Enterprise.Core.dll" culture="neutral"/>
        <codeBase href="plugins\ClearCanvas.Enterprise.Core.dll"/>
