<ccUI:ToolbarButton runat="server" SkinID="EditButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/EditEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/EditDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="ResetButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/ResetEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/ResetDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="RescheduleButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/RescheduleEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/RescheduleDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="AddButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/AddEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/AddDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="DoneButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/DoneEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/DoneDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="SearchButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/SearchEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/SearchDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="OKButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/OKEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/OKDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="CancelButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/CancelEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/CancelDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="YesButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/YesEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/YesDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="NoButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/NoEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/NoDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="ApplyButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/ApplyEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/ApplyDisabled.png" />
<asp:LinkButton runat="server" SkinId="CloseButton" Text="Close" CssClass="CloseButton" />

<%-- Tables --%>

<asp:table runat="server" CellPadding="0" BorderWidth="0" CellSpacing="0" Width="100%"/>
<asp:table runat="server" SkinId="MainContentTable" CellPadding="0" BorderWidth="0" CellSpacing="0" Width="100%" CssClass="MainContentTable"/>

<%-- Images --%>
<asp:Image runat="server" SkinID="Unchecked" ImageUrl="~/App_Themes/Default/images/Indicators/unchecked.png" />
