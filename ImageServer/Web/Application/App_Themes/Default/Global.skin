<%@ Register assembly="ClearCanvas.ImageServer.Web.Common" namespace="ClearCanvas.ImageServer.Web.Common.WebControls.UI" tagPrefix="ccUI" %>
<%@ Register src="~/Controls/InvalidInputIndicator.ascx" tagPrefix="ccASP" tagName="InvalidInputIndicator" %>

<asp:Image runat="server" SkinID="Spacer" ImageUrl="~/images/blank.gif" />

<%-- Header and Menu Images --%>

<asp:ImageButton runat="server" SkinID="Logo" ImageUrl="images/Header/ClearCanvasLogo.png" Height="68" Width="199" />
<asp:Image runat="server" SkinID="MenuEdge" ImageUrl="images/Header/MenuEdge.png" Height="39" Width="22"/>
<asp:Image runat="server" SkinID="MainContentBottomLeftCorner" ImageUrl="images/MainContent/MainContentBottomLeftCorner.png" Height="10" Width="7"/>
<asp:Image runat="server" SkinID="MainContentBottomRightCorner" ImageUrl="images/MainContent/MainContentBottomRightCorner.png" Height="10" Width="7" /> 

<asp:Menu runat="server" SkinID="MainMenu" CssClass="MainMenuContent" Orientation="Horizontal"
          StaticSubMenuIndent="1" DynamicVerticalOffset="0" DynamicHorizontalOffset="0" StaticEnableDefaultPopOutImage="false">
    <DynamicMenuStyle CssClass="DynamicMenu" BackColor="#3482b2"/>
    <DynamicHoverStyle CssClass="DynamicHover" />
    <DynamicMenuItemStyle CssClass="DynamicMenuItem" />
    <DynamicSelectedStyle CssClass="DynamicSelected" />
    <StaticHoverStyle CssClass="StaticHover" />
    <StaticMenuItemStyle CssClass="StaticMenuItem" />
    <StaticMenuStyle CssClass="StaticMenu" />
    <StaticSelectedStyle CssClass="StaticSelected" />
</asp:Menu>

<%-- Buttons --%>

<asp:ImageButton runat="server" SkinID="SearchButton" ImageUrl="images/Buttons/Search.png" />
<ccUI:ToolbarButton runat="server" SkinID="ViewDetailsButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/ViewDetailsEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/ViewDetailsDisabled.png" />
<ccUI:ToolbarButton runat="server" SkinID="MoveButton" EnabledImageURL="~/App_Themes/Default/images/Buttons/MoveEnabled.png" DisabledImageURL="~/App_Themes/Default/images/Buttons/MoveDisabled.png" />
