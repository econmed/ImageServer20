<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="ClearCanvas.Dicom.Services" namespace="ClearCanvas.Dicom.Services">
  <class name="Series" proxy="Series" table="Series_">
    <id name="Oid" column="Oid_" type="Int64">
      <generator class="hilo">
        <param name="max_lo">100</param>
      </generator>
    </id>
    <version name="Version" column="Version" />
    <property name="SeriesInstanceUid" column="SeriesInstanceUid_" type="String" length="64" />
	<property name="Modality" column="Modality_" type="String" length="16" />
	<property name="SeriesNumber" column="SeriesNumber_" type="Int32" />
	<property name="Laterality" column="Laterality_" type="String" length="16" />
	<property name="SeriesDescription" column="SeriesDescription_" type="String" length="64" />
    <set name="SopInstances" lazy="true" inverse="true" cascade="all-delete-orphan" access="nosetter.camelcase-underscore">
      <key column="SeriesOid_" />
      <one-to-many class="SopInstance" />
    </set>
  </class>
</hibernate-mapping>
