<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ClearCanvas.Dicom.DataStore" namespace="ClearCanvas.Dicom.DataStore">
  <class name="DicomDictionaryContainer" table="DicomDictionaryContainer_" lazy="false">
    <id name="EntryOid" column="EntryOid_" type="Guid">
      <generator class="guid.comb">
      </generator>
    </id>
    <property name="DictionaryName" column="DictionaryName_" type="string" length="256" not-null="true" unique="true" />
    <list name="DictionaryEntries" table="DictionaryEntries_" lazy="false" access="nosetter.camelcase-underscore">
		<key column="EntryOid_" />
		<index column="Index_" />
		<composite-element class="ClearCanvas.Dicom.DataStore.DictionaryEntry, ClearCanvas.Dicom.DataStore">
      <property name="TagName" column="TagName_" type="string" length ="64" />
      <nested-composite-element name="Path" class="DicomTagPath, ClearCanvas.Dicom.DataStore">
        <property name="Path" column="Path_" type="string" length="256" />
      </nested-composite-element>
      <property name="IsComputed" column="IsComputed_" type="Boolean" />
      <property name="ValueRepresentation" column="ValueRepresentation_" type="string" length ="16" />
		</composite-element>
	</list>
  </class>
</hibernate-mapping>
