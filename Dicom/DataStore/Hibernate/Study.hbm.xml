<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="ClearCanvas.Dicom.DataStore" namespace="ClearCanvas.Dicom.DataStore">
  <class name="Study" table="Study_">
    <id name="StudyOid" column="StudyOid_" type="Int64">
      <generator class="hilo">
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="ProcedureCodeSequenceCodingSchemeDesignator" column="ProcedureCodeSequenceCodingSchemeDesignator_" type="string" length="16" />
	<property name="StudyId" column="StudyId_" type="string" length="16" />
	<property name="StudyTime" column="StudyTime_" type="string" length="16" />
	<property name="StudyDate" column="StudyDate_" type="string" length="26" />
	<property name="AccessionNumber" column="AccessionNumber_" type="string" length="16" />
	<property name="StudyInstanceUid" column="StudyInstanceUid_" type="string" length="64" />	
	<property name="StudyDescription" column="StudyDescription_" type="string" length="64" />
	<property name="ProcedureCodeSequenceCodeValue" column="ProcedureCodeSequenceCodeValue_" type="string" length="16" />
    <component name="PatientsName" class="ClearCanvas.Dicom.PersonName, ClearCanvas.Dicom"> <!-- class attribute optional -->
		<property name="InternalPersonName" column="PatientsName_" type="string" length="256" />
    </component>
	<component name="PatientId" class="ClearCanvas.Dicom.PatientId, ClearCanvas.Dicom">
		<property name="InternalPatientId" column="PatientId_" type="string" length="64" />
	</component>
	<property name="PatientsSex" column="PatientsSex_" type="string" length="16" />
	<property name="PatientsBirthDate" column="PatientsBirthDate_" type="string" length="16" />
    <set name="InternalSeries" table="Series_" lazy="false" inverse="true" cascade="all-delete-orphan" access="nosetter.camelcase-underscore">
      <key column="StudyOid_" />
      <one-to-many class="Series" />
    </set>
  </class>
</hibernate-mapping>
