<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ClearCanvas.Healthcare" namespace="ClearCanvas.Healthcare">
  <class name="MimeDocument" proxy="MimeDocument" table="MimeDocument_">
    <id name="OID" column="OID_" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <version name="Version" column="Version_"/>

    <property name="MimeType" column="MimeType_" type="string" length="32" not-null="true"/>

    <property name="CreationTime" column="CreationTime_" type="DateTime" not-null="true"/>

    <many-to-one name="Data" column="MimeDocumentDataOID_" class="MimeDocumentData" not-null="true" cascade="all" unique="true"/>

  </class>
</hibernate-mapping>
