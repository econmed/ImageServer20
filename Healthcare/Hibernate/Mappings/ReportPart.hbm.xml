<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="ClearCanvas.Healthcare" namespace="ClearCanvas.Healthcare">
  <class name="ReportPart" proxy="ReportPart" table="ReportPart_">
    <id name="OID" column="OID_" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <version name="Version" column="Version_"/>

    <property name="Id" column="Id_" type="String" length="30" not-null="true"/>
    <property name="Content" column="Content_" type="StringClob" length="65000" not-null="false"/>

    <many-to-one name="Report" class="Report" column="ReportOID_" not-null="true" fetch="join"/>
  </class>
</hibernate-mapping>
