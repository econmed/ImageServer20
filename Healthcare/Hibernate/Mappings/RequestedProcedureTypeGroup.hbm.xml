<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ClearCanvas.Healthcare" namespace="ClearCanvas.Healthcare">
  <class name="RequestedProcedureTypeGroup" proxy="RequestedProcedureTypeGroup" table="RequestedProcedureTypeGroup_">
    <cache usage="nonstrict-read-write"/>
    <id name="OID" column="OID_" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <version name="Version" column="Version_"/>

    <property name="Name" column="Name_" type="String" length="100" not-null="true" unique="true"/>
    <property name="Description" column="Description_" type="String" length="1024" not-null="false"/>
    <property name="Category" column="Category_" type="ClearCanvas.Healthcare.Hibernate.RequestedProcedureTypeGroupCategoryEnumHbm, ClearCanvas.Healthcare.Hibernate" not-null="true"/>

    <set name="RequestedProcedureTypes" table="RequestedProcedureTypeGroupRequestedProcedureType_" lazy="true" cascade="all" access="nosetter.camelcase-underscore">
      <key column="RequestedProcedureTypeGroupOID_"/>
      <many-to-many column="RequestedProcedureTypeOID_" class="RequestedProcedureType" fetch="join"/>
    </set>
  </class>
</hibernate-mapping>
