<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ClearCanvas.Healthcare" namespace="ClearCanvas.Healthcare">
  <class name="DiagnosticServiceTreeNode" proxy="DiagnosticServiceTreeNode" table="DiagnosticServiceTree_">
    <cache usage="nonstrict-read-write"/>
    <id name="OID" column="OID_" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <version name="Version" column="Version_"/>

    <property name="Description" column="Description_" type="String" length="64" not-null="true" unique="false"/>
    <many-to-one name="DiagnosticService" class="DiagnosticService" column="DiagnosticServiceOID_" not-null="false"/>

    <many-to-one name="Parent" class="DiagnosticServiceTreeNode" column="ParentOID_" not-null="false" fetch="join"/>
    <list name="Children" lazy="true" cascade="all-delete-orphan" access="nosetter.camelcase-underscore">
      <key column="ParentOID_"/>
      <index column="DisplayOrder_"/>
      <one-to-many class="DiagnosticServiceTreeNode"/>
    </list>

    </class>
</hibernate-mapping>
