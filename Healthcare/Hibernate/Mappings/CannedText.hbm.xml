<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ClearCanvas.Healthcare" namespace="ClearCanvas.Healthcare">
  <class name="CannedText" proxy="CannedText" table="CannedText_">
	<id name="OID" column="OID_" type="Guid">
	  <generator class="guid.comb"/>
	</id>
	<version name="Version" column="Version_"/>

	<property name="Name" column="Name_" type="String" length ="1024" not-null="true"/>
	<property name="Path" column="Path_" type="String" length ="1024"/>
	<property name="Text" column="Text_" type="StringClob" length ="65000" not-null="true" />

	<many-to-one name="Staff" class="Staff" column="StaffOID_" fetch="join"/>
	<many-to-one name="StaffGroup" class="StaffGroup" column="StaffGroupOID_" fetch="join"/>

  </class>
</hibernate-mapping>
