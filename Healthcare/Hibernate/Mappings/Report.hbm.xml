<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ClearCanvas.Healthcare" namespace="ClearCanvas.Healthcare">
  <class name="Report" proxy="Report" table="Report_">
    <id name="OID" column="OID_" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <version name="Version" column="Version_"/>

    <property name="Status" column="Status_" type="ClearCanvas.Healthcare.Hibernate.ReportStatusEnumHbm, ClearCanvas.Healthcare.Hibernate" not-null="true"/>
    
    <set name="Procedures" table="RequestedProcedureReport_" lazy="true">
      <key column="ReportOID_"/>
      <many-to-many column="ProcedureOID_" class="RequestedProcedure" fetch="join"/>
    </set>

    <list name="Parts" lazy="true" inverse="true" cascade="all-delete-orphan" access="nosetter.camelcase-underscore">
      <key column="ReportOID_"/>
      <index column="Index_"/>
      <one-to-many class="ReportPart"/>
    </list>

  </class>
</hibernate-mapping>
